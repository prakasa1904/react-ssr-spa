{
  "name": "react-ssr-spa",
  "private": false,
  "version": "10.0.0",
  "description": "Simple server rendered single page app.",
  "repository": "https://github.com/michaelBenin/react-ssr-spa",
  "license": "MIT",
  "author": "Michael Benin",
  "main": "./dist/server/index.js",
  "scripts": {
    "postinstall": "gulp vendor-js babel-server",
    "start": "NODE_ENV=development gulp dev",
    "client-unit-test": "gulp client-unit-test",
    "client-integration-test": "gulp client-integration-test",
    "client-unit-test-watch": "KARMA_AUTOWATCH=on gulp client-unit-test",
    "client-integration-test-watch": "KARMA_AUTOWATCH=on gulp client-integration-test",
    "server-unit-test": "gulp server-unit-test",
    "server-integration-test": "gulp server-integration-test",
    "acceptance-test": "rm -rf ./dist && NODE_ENV=production npm run build-prod; NODE_ENV=test gulp acceptance-test",
    "js-lint": "gulp js-lint",
    "js-lint-fix": "gulp js-lint-fix",
    "style-lint": "gulp style-lint",
    "style-lint-fix": "stylelint ./src/**/*.scss --syntax scss --config gulpfile.babel.js/configs/.stylelintrc --fix",
    "fix-all": "npm run js-lint-fix && npm run style-lint-fix",
    "svg-min": "gulp svg-min",
    "test": "NODE_ENV=test gulp test",
    "ci-test": "gulp ci-test",
    "prod-server": "pm2 reload pm2/pm2.json5",
    "build-prod": "NODE_ENV=production gulp build-prod",
    "create-upload-artifact": "gulp create-upload-artifact",
    "upload-static-files": "gulp upload-static-files upload-vendor-file",
    "clean": "gulp clean",
    "styles-doc": "gulp style-doc",
    "js-doc": "gulp js-doc",
    "complexity-report": "plato -r -d docs/complexity src",
    "docs": "gulp js-doc style-doc",
    "coverage": "cross-env NODE_ENV=coverage nyc mocha --timeout 10000 test/server/utils/initializer_util.js test/server/utils/test_initializer_util.js test/server/integration/**/*.js test/server/utils/enzyme_initializer.js test/server/unit/**/*.js test/server/utils/test_teardown_util.js && cat ./docs/server-coverage/lcov.info | coveralls && rm -rf ./docs/server-coverage/lcov.info .nyc_output",
    "client-coverage": "gulp client-coverage; cat docs/client-coverage/lcov.info | codecov",
    "analyze-bundle": "source-map-explorer dist/static/js/{}.js dist/static/js/{}.js.map"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "dotenv": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "exenv": "^1.2.2",
    "express": "^4.16.3",
    "express-bunyan-logger": "^1.3.3",
    "express-content-length-validator": "^1.0.0",
    "express-enforces-ssl": "^1.1.0",
    "express-graceful-exit": "^0.4.1",
    "express-session": "^1.15.6",
    "fontfaceobserver": "^2.0.13",
    "glob": "^7.1.2",
    "he": "^1.1.1",
    "helmet": "^3.12.2",
    "hiredis": "^0.5.0",
    "history": "^4.7.2",
    "hpp": "^0.2.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-error-boundary": "^1.2.3",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-transition-group": "^2.4.0",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "request": "^2.87.0",
    "require-dir": "^1.0.0",
    "response-time": "^2.3.2",
    "scriptjs": "^2.5.8",
    "serialize-javascript": "^1.5.0",
    "source-map-support": "^0.5.6",
    "statuses": "^1.5.0",
    "uuid": "^3.3.2",
    "validate.js": "^0.12.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bunyan-prettystream": "^0.1.3",
    "chai": "^4.1.2",
    "chai-enzyme": "^0.8.0",
    "cheerio": "^1.0.0-rc.1",
    "codecov": "^3.0.4",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "enzyme": "^2.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.4.4-rc1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-aws": "^0.0.5",
    "gulp-awspublish": "^3.3.3",
    "gulp-babel": "^7.0.1",
    "gulp-changed": "^3.2.0",
    "gulp-esdoc": "^0.4.1",
    "gulp-eslint": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-if": "^2.0.2",
    "gulp-livereload": "^3.8.1",
    "gulp-mocha": "^5.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.6.5",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylelint": "^6.0.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-tar": "^2.1.0",
    "gulp-util": "^3.0.8",
    "gulp-webdriver": "^2.0.3",
    "jsdom": "^11.6.2",
    "karma": "^2.0.0",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.2.0",
    "mocha.parallel": "^0.15.5",
    "normalize-scss": "^7.0.1",
    "nyc": "^11.9.0",
    "plato": "^1.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.4.1",
    "run-sequence": "^2.2.1",
    "sassdoc": "^2.5.0",
    "selenium-standalone": "^6.15.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "source-map-explorer": "^1.5.0",
    "stylelint": "^8.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.5.0",
    "supertest": "^3.1.0",
    "type-detect": "^4.0.8",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "^0.0.9",
    "webdriverio": "^4.13.1",
    "webpack": "^3.12.0",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-common-shake": "^1.5.3",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "engines": {
    "node": "^8.11.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "src/client/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "report-dir": "./docs/server-coverage"
  }
}
